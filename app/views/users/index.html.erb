<h1>Index of Users</h1>
<table>
<% @users.each do |user| %>
<tr> <%= user.username %> </tr>
<tr> <%= link_to 'Show ', user_path(user.id) %> </tr>
<tr> <%= link_to 'Edit ', edit_user_path(user.id) %> </tr>
<tr> <%= link_to 'Delete ', user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %> </tr>

<br/>

<% end %>
</table>
<%= link_to 'New', new_user_path %>
