<%= @user.username %>, <%= @user.email %>
<% provide(:title, @user.username) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <tr> <%= @user.username %> </tr>
        <tr> <%= link_to 'Edit ', edit_user_path(@user.id) %> </tr>
        <tr> <%= link_to 'Delete ', user_path(@user), method: :delete, data: { confirm: 'Are you sure?' } %> </tr>

      </h1>
    </section>
  </aside>
</div>
